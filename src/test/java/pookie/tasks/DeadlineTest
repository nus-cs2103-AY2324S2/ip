package pookie.tasks;  //same package as the class being tested

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class DeadlineTest {
    @Test
    public void testToString(){
        assertEquals("[D][ ] return book (by: Feb 12 2019 18:00)", new Deadline("return book", "2019-02-12 18:00").toString());
    }

    @Test
    public void testWriteToFileString(){
        assertEquals("Deadline return book /by 2019-02-12 18:00", new Deadline("return book", "2019-02-12 18:00").writeToFileString());
    }
}