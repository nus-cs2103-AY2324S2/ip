<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox styleClass="dialog-container" minWidth="75.00" maxWidth="1.7976931348623157E308" maxHeight="1.7976931348623157E308">
            <Text fx:id="content" styleClass="dialog-text" wrappingWidth="180.0"/>
        </VBox>
        <VBox minWidth="7.0"></VBox>
        <ImageView fx:id="displayPicture"  styleClass="dialog-image" fitHeight="99.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" />

    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
