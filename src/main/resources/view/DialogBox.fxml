<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Rectangle fx:id="textBackground" arcHeight="20" arcWidth="20" fill="#cce6ff" HBox.hgrow="ALWAYS"/>
        <Label fx:id="dialog" graphicTextGap="20.0" text="Label" wrapText="true" textOverrun="CLIP" >
            <font>
                <Font name="Baloo Da 2 SemiBold" size="12.0" />
            </font>
                <padding>
                    <Insets top="7.0" right="7.0" bottom="7.0" left="7.0"/>
                </padding>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="70.0" fitWidth="70.0" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
