<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
            <Label fx:id="dialog" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="Label" HBox.hgrow="ALWAYS">
                <font>
                    <Font name="Lato" size="14.0" />
                </font>
                <HBox.margin>
                    <Insets bottom="10.0" left="4.0" right="4.0" top="10.0" />
                </HBox.margin>
                <padding>
                    <Insets bottom="4.0" left="10.0" right="10.0" top="4.0" />
                </padding>
                <VBox.margin>
                    <Insets left="8.0" right="8.0" top="10.0" />
                </VBox.margin>
            </Label>
            <Label fx:id="infoLabel" opacity="0.57" text="Username [12:34 PM]">
                <font>
                    <Font size="10" />
                </font>
                <VBox.margin>
                    <Insets bottom="3.0" left="10.0" right="10.0" />
                </VBox.margin>
                <padding>
                    <Insets left="10.0" right="10.0" />
                </padding>
            </Label>

        </VBox>

        <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="52.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
