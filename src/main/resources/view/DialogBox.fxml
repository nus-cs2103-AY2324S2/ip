<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="1.7976931348623157E308" prefWidth="400.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
         style="-fx-background-color: #b49577">
    <children>
            <Label fx:id="dialog" text="Label" wrapText="true" VBox.vgrow="ALWAYS"
                   style="-fx-background-radius: 7.5;-fx-background-color: #e0cab8;-fx-font-family: Verdana; -fx-font-size: 12px">
                <padding>
                    <Insets bottom="3.0" left="7.0" right="7.0" top="3.0"/>
                </padding>
            </Label>
        <ImageView fx:id="displayPicture" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets left="10.0" right="10.0"/>
            </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
