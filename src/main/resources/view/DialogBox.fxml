    <children>
        <Label fx:id="dialog" alignment="CENTER_RIGHT" contentDisplay="RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
