            <InnerShadow />
         </effect></ImageView>
        <Label fx:id="dialog" lineSpacing="1.0" style="-fx-background-color: #F3F3F3;" text="Label" wrapText="true" minHeight="-Infinity">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin></Label>
      <Circle fx:id="circle" fill="DODGERBLUE" radius="49.0" stroke="#fff9f9" strokeType="INSIDE" strokeWidth="0.0" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
