<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="Infinity" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity" maxHeight="Infinity">
      <padding>
          <Insets top="0.0" right="5.0" bottom="0.0" left="5.0" />
      </padding>
    </Label>
    <ImageView fx:id="displayPicture" fitHeight="80.0" fitWidth="80.0" />
  </children>
  <padding>
    <Insets top="15.0" right="5.0" bottom="15.0" left="5.0" />
  </padding>
</fx:root>
