<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<fx:root id="dialogBox" alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" prefHeight="162.0" prefWidth="350.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text id="dialogTextShape" fx:id="dialogTextShape" fill="#c9c9c9" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Text" wrappingWidth="150.0"/>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets left="8.0" right="8.0"/>
            </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="10.0" right="10.0" top="15.0"/>
    </padding>
</fx:root>
