<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" styleClass="dialog-box">
    <children>
        <StackPane styleClass="dialog-text-pane">
            <children>
                <Label fx:id="dialog" text="Label" wrapText="true" />
            </children>
        </StackPane>
        <ImageView fx:id="displayPicture" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
            <clip>
                <Circle radius="30.0" centerX="30.0" centerY="30.0" />
            </clip>
            <HBox.margin>
                <Insets top="10.0" right="10.0" bottom="10.0" left="10.0" />
            </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</fx:root>
