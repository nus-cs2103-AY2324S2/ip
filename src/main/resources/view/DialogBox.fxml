<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>


<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" minHeight="-Infinity" text="arial" wrapText="true">
            <font>
                <Font name="Arial" size="12.0" />
            </font>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
            <clip>
                <Circle radius="28" centerX="28" centerY="28" />
            </clip>
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
            <effect>
                <javafx.scene.effect.DropShadow color="#000000" spread="0.9" />
            </effect>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="10.0" right="15.0" top="22.0" />
    </padding>
</fx:root>