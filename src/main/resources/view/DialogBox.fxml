<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <StackPane>
            <Rectangle fx:id="textBackground" arcHeight="20" arcWidth="20"/>
            <Label fx:id="dialog" graphicTextGap="20.0" prefWidth="238.0" text="Label" textFill="#726363"
                   wrapText="true" textOverrun="CLIP">
             <font>
                <Font name="Helvetica Neue Bold" size="12.0" />
             </font>
                <padding>
                    <Insets top="10.0" right="10.0" bottom="10.0" left="10.0"/>
                </padding>
          </Label>
        </StackPane>
        <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true">
         <HBox.margin>
            <Insets left="15.0" right="15.0" />
         </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
